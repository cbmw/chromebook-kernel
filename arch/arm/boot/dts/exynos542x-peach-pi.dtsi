/ {
	pinctrl@13400000 {
		backlight_en: backlight-en {
			samsung,pins = "gpx2-2";
			samsung,pin-function = <1>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	fimd {
		samsung,fimd-src-clk-rate = <150000000>;
	};

	lcd_dp0: lcd_panel0 {
		lcd-htiming = <172 60 80 1920>;
		lcd-vtiming = <25 10 10 1080>;
		lcd-dimensions = <360 202>;
	};

	display-port-controller {
		samsung,link-rate = <0x0a>;
	};

	backlight {
		enable-gpio = <&gpx2 2 0>;
		pinctrl-0 = <&backlight_pwm &backlight_en>;
	};

	usb3_phy0: usbphy@12100000 {
		samsung,txpreempamptune = <0x1>;
	};

	usb3_phy1: usbphy@12500000 {
		samsung,txpreempamptune = <0x2>;
	};

	/*hsi2c7*/
	hsi2c@12CD0000 {
		max98090: codec@10 {
			compatible = "maxim,max98091";
		};
	};

	spi@12d40000 {
		cros-ec@0 {
			i2c-tunnel {
				power-regulator@48 {
						regulators {
							fet1 {
								regulator-always-on;
							};
							fet6 {
								regulator-always-on;
							};
						};
				};
			};
		};
	};

	sound {
		compatible = "google,daisy-audio-max98091";
	};
};
